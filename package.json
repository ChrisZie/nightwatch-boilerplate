{
  "name": "nightwatch-Boilerplate",
  "description": "nightwatch boilerplate with selenium testing",
  "version": "0.0.1",
  "author": {
    "name": "Christine Zierold"
  },
   "repository": {
    "type": "git",
    "url": "git@github.com/christinezierold27/nightwatch-boilerplate.git"
