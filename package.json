    "phantomjs-prebuilt": "^2.1.7",
    "selenium-server": "^2.53.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^1.2.0",
    "mocha": "^2.5.3",
    "nightwatch-cucumber": "^4.0.0"
  },
  "bin": {
    "nightwatch": "./node_modules/nightwatch/bin/nightwatch"
  }
}
